  "browserslist": [
    "> 5%",
    "last 2 version",
    "not dead"
  ],
  "stylelint": {
    "extends": [
      "stylelint-prettier/recommended"
    ]
  },
  "dependencies": {
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "postcss-preset-env": "^6.7.0"
  }
