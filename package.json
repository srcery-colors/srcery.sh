{
  "name": "srcery.github.io",
  "version": "2.4.3",
  "private": true,
  "description": "Web site for promoting the Srcery color scheme.",
  "repository": {
    "type": "git",
    "url": "https://github.com/srcery-colors/srcery.git"
  },
  "license": "MIT",
  "author": "Srcery Team",
  "main": "src/index.html",
  "scripts": {
    "build": "parcel build src/index.html",
    "clean": "rm -fr ./.cache ./dist",
    "serve": "parcel src/index.html --open",
